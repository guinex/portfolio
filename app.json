{
  "addons": [
    "heroku-postgresql",
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "env": {
    "ENABLE_CONTACT": {
      "required": true
    },
    "HEROKU_POSTGRESQL_MAROON_URL": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "PORTFOLIO_URL": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "portfolio",
  "scripts": {
    "pr-predestroy": "rake db:migrate"
  },
  "stack": "heroku-18"
}
